{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'Header',\n  data() {\n    return {\n      // Flag per determinare se la navbar è aperta o chiusa\n      isNavbarOpen: false\n    };\n  },\n  methods: {\n    async toggleNavbar() {\n      this.isNavbarOpen = !this.isNavbarOpen;\n    },\n    async goHome() {\n      this.$router.push('/');\n    },\n    async logout() {\n      try {\n        let token = localStorage.getItem('token').replace(/\"/g, ''); // Rimuovo le virgolette\n        console.log(token);\n        let response = await axios.get(`https://phplaravel-729699-3062059.cloudwaysapps.com/api/v1/logout`, {\n          headers: {\n            'Authorization': `Bearer ${token}`\n          }\n        });\n        console.log(response);\n        if (response.status == 200) {\n          localStorage.clear();\n          this.$router.push({\n            name: 'Login'\n          });\n        } else {\n          console.warn(\"Logout non riuscito.\");\n        }\n      } catch (error) {\n        console.error(\"Errore durante il logout:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","isNavbarOpen","methods","toggleNavbar","goHome","$router","push","logout","token","localStorage","getItem","replace","console","log","response","get","headers","status","clear","warn","error"],"sources":["/home/ilaria/kanbanapp/src/components/Header.vue"],"sourcesContent":["<template>\n    <nav class=\"navbar navbar-expand-lg navbar-light\" style=\"background-color: #022970;\">\n        <div class=\"container-fluid\">\n            <!-- Toggler per dispositivi mobili -->\n            <button class=\"navbar-toggler\" type=\"button\" @click=\"toggleNavbar\"\n                aria-controls=\"navbarTogglerDemo03\" :aria-expanded=\"isNavbarOpen ? 'true' : 'false'\"  \n                aria-label=\"Toggle navigation\" \n                style=\"color: #ffffff; background-color: #022970; border-color: #ffffff; filter: opacity(50%);\"> <!--Ho dovuto gestire la chiusura con isNavbarOpen perchè bootstrap entrava in conflitto con vuejs e il menù una volta aperto non si chiudeva-->\n                <span class=\"navbar-toggler-icon\" style=\"filter: invert(100%);\"></span>\n            </button>\n            <!-- Navbar Menu -->\n            <div class=\"navbar-collapse\" :class=\"{'collapse': !isNavbarOpen, 'show': isNavbarOpen}\" id=\"navbarTogglerDemo03\">\n                <ul class=\"navbar-nav me-auto mb-2 mb-lg-0 mt-3\" style=\"width: 90%\">\n                    <li class=\"nav-item\">\n                        <a id=\"home\" class=\"nav-link active bi bi-house-door rounded-1 mb-2\" aria-current=\"page\"\n                            @click=\"this.$router.push({ name: 'Home' });\" style=\"color: #ffffff;\">&nbsp;Home</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a id=\"Client\" class=\"nav-link active bi bi-person rounded-1 mb-2\" aria-current=\"page\"\n                            @click=\"this.$router.push({ name: 'Client' });\" style=\"color: #ffffff;\">&nbsp;Client</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a id=\"openOrders\" class=\"nav-link active bi bi-box-seam rounded-1 mb-2\" aria-current=\"page\"\n                            href=\"/#open-orders\" style=\"color: #ffffff;\">&nbsp;Open orders</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a id=\"productionPlanState\" class=\"nav-link active bi bi-kanban rounded-1 mb-2\" aria-current=\"page\"\n                            @click=\"this.$router.push({ name: 'Facilities' });\" style=\"color: #ffffff;\">&nbsp;Stato impianti</a>\n                    </li>\n                    <li class=\"nav-item\">\n                        <a id=\"logout\" class=\"nav-link bi bi-box-arrow-right rounded-1 mb-2\" @click=\"logout\"\n                        