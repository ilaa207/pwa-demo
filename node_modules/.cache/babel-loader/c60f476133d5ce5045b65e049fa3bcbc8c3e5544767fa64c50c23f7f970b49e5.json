                   this.$router.push({ name: 'Client' });\n                } else {\n                    console.warn(\"Login failed, invalid data..\");\n                }\n\n            } catch (error) {\n                /*if (this.email.trim() == '' || this.password.trim() == '') { //.trim rimuove gli spazi e gli a capo, controllo se sono vuoti e se uno dei due lo è mando un messaggio di errore*/\n                this.errorMessage = 'E-mail o password errati.';\n                console.error(\"Error while logging in:\", error);\n            }\n        }\n    },\n\n//controllo che l'utente sia loggato, lo faccio tramite l'id salvato e se è loggato, rimando alla pagina di di scelta cliente\nmounted() {\n    let userId = localStorage.getItem('id')\n    if (userId) {\n        this.$router.push({ name: 'Client' })\n    }\n}\n}\n</script>\n\n"],"mappings":";AA+BA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,MAAK,MAAO,YAAY;AAC/B,eAAe;EACXC,IAAI,EAAE,OAAO;EAEbC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE;IAClB;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAG;MACnB,IAAIC,EAAC,GAAIC,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC;MAC5C,IAAIF,EAAE,CAACG,IAAG,KAAM,UAAU,EAAE;QACxBH,EAAE,CAACG,IAAG,GAAI,MAAM;MACpB,OAAO;QACHH,EAAE,CAACG,IAAG,GAAI,UAAU;MACxB;IACJ,CAAC;IACO,MAAMC,KAAKA,CAAA,EAAG;MACV,IAAI;QACA,MAAMb,KAAK,CAACc,GAAG,CAAC,kBAAkB,EAAE;UAChCV,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;QAEFU,OAAO,CAACC,GAAG,CAACC,QAAQ,CAAC;QACrB,IAAIA,QAAQ,CAACC,MAAK,IAAK,GAAE,IAAKC,MAAM,CAACC,IAAI,CAACH,QAAQ,CAACd,IAAI,CAAC,CAACkB,MAAK,GAAI,CAAC,EAAE;UAAE;UACnEC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEC,IAAI,CAACC,SAAS,CAACR,QAAQ,CAACd,IAAI,CAACuB,KAAK,CAAC,CAAC;UAClEJ,YAAY,CAACC,OAAO,CAAC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACR,QAAQ,CAACd,IAAI,CAACwB,IAAI,CAACC,EAAE,CAAC,CAAC;UACjEN,YAAY,CAACC,OAAO,CAAC,cAAc,EAAEC,IAAI,CAACC,SAAS,CAACR,QAAQ,CAACd,IAAI,CAACwB,IAAI,CAACE,YAAY,CAAC,CAAC;UACrFP,YAAY,CAACC,OAAO,CAAC,gBAAgB,EAAEC,IAAI,CAACC,SAAS,CAACR,QAAQ,CAACd,IAAI,CA