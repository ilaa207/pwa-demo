onse = await axios.get(`https://phplaravel-729699-3062059.cloudwaysapps.com/api/v1/customer/${this.customer.id}/orders`, {\n          headers: {\n            'authorization': `Bearer ${token}`,\n            'accept': 'application/json',\n          },\n        });\n        console.log(response);\n        this.orders = response.data.orders;\n      } catch (error) {\n        console.warn(\"Error: \", error)\n      }\n    },\n    selectedOrder(order) {\n      localStorage.setItem('selectedOrder', JSON.stringify({\n        id: order.id,\n        items: order.order_items\n      }));\n      this.$router.push({ name: 'OpenOrder' });\n    }\n  },\n\n\n\n  //controllo che l'utente sia loggato, lo faccio tramite l'id salvato e se non è loggato, rimando alla pagina di login\n  mounted() {\n    let userId = localStorage.getItem('id');\n    console.log(localStorage.getItem('id'));\n\n    if (userId == null) {\n      this.$router.push({ name: 'Login' })\n    }\n    // Recupera il cliente da localStorage\n    this.customer = JSON.parse(localStorage.getItem('selectedCustomer'));\n    // Recupera gli ordini aperti\n    if (this.customer.id) {\n      this.openOrders();\n    }\n  },\n};\n</script>"],"mappings":";AAoCA,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MAAE;MACdC,MAAM,EAAE,EAAE,CAAE;IACd,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVN;EACF,CAAC;EACDO,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAIC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,CAACC,OAAO,CAAC,IAAI,EAAE,EAAE,CAAC;MAC3D,IAAI;QACF,IAAIC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,uEAAuE,IAAI,CAACV,QAAQ,CAACW,EAAE,SAAS,EAAE;UAC/HC,OAAO,EAAE;YACP,eAAe,EAAE,UAAUP,KAAK,EAAE;YAClC,QAAQ,EAAE;UACZ;QACF,CAAC,CAAC;QACFQ,OAAO,CAACC,GAAG,CAACL,QAAQ,CAAC;QACrB,IAAI,CAACR,MAAK,GAAIQ,QAAQ,CAACV,IAAI,CAACE,MAAM;MACpC,EAAE,OAAOc,KAAK,EAAE;QACdF,OAAO,CAACG,IAAI,CAAC,SAAS,EAAED,KAAK;MAC/B;IACF,CAAC;IACDE,aAAaA,CAACC,KAAK,EAAE;MACnBZ,YAAY,CAACa,OAAO,CAAC,eAAe,EAAEC,